{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "esModuleInterop": true,
    "target": "ES2015",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@ui/*": ["src/components/ui/*"],
      "@assets/*": ["src/assets/*"],
      "@routes/*": ["src/routes/*"]
    }
  },
  "include": ["src"],
  "references": []
}